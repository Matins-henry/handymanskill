{
  "version": 2,
  "buildCommand": "cd SkillMatchPro && npm install && npm run build",
  "outputDirectory": "SkillMatchPro/client/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
